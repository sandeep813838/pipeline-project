pipeline {
    agent any
    stages {
          stage('Git Checkout'){
            steps{
            gitCheckout(
                branch: "main",
                url: "https://github.com/sandeep813838/pipeline-project.git"
            )
            }
          }
        stage(‘Build’) {
            steps {
                sh 'git --version'
            }
        }
    }
}
